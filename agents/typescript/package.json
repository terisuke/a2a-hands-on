{
  "name": "a2a-agents-typescript",
  "version": "1.0.0",
  "description": "A2A Handson TypeScript Agents",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:namer": "node dist/namer/index.js",
    "start:commit": "node dist/commit/index.js",
    "start:reviewer": "node dist/reviewer/index.js",
    "start:documenter": "node dist/documenter/index.js",
    "dev:namer": "tsx src/namer/index.ts",
    "dev:commit": "tsx src/commit/index.ts",
    "dev:reviewer": "tsx src/reviewer/index.ts",
    "dev:documenter": "tsx src/documenter/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "openai": "^4.28.0",
    "@google/generative-ai": "^0.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20"
  }
}
